<%= form_with(url: sessions_path, local: true) do |form| %>
  <%= render 'shared/flash' %>

  <div class="field">
    <%= form.label :email %>
    <%= form.text_field :email, id: :user_email %>
  </div>
  
  <div class="field">
    <%= form.label :password %>
    <%= form.password_field :password, id: :user_password %>
  </div>

  <div class="actions">
    <%= form.submit 'Enter' %>
  </div>
<% end %>
