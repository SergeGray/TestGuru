<tr>
  <td class="align-middle"><%= test.title %></td>
  <td class="align-middle"><%= test.category.title %></td>
  <td class="align-middle"><%= test.level %></td>
  <td class="align-middle">
    <%= test.author.first_name %> <%= test.author&.last_name %>
  </td>
  <td class="align-middle"><%= test.questions.count %></td>
  <td class="align-middle"><%= button_to t('.start'),
                         start_test_path(test),
                         class: 'btn btn-secondary' %></td>
</tr>
